{
  "entrypoint": "polymer-vis.html",
  "fragments": [],
  "sources": ["src/**/*", "polymer-vis.html"],
  "extraDependencies": ["bower_components/webcomponentsjs/*.js"],
  "builds": [
    {
      "name": "es5",
      "js": {
        "minify": true,
        "compile": false
      },
      "css": {
        "minify": true
      },
      "html": {
        "minify": true
      },
      "addServiceWorker": false,
      "addPushManifest": false
    }
  ],
  "lint": {
    "rules": ["polymer-2"]
  }
}
