{
  "$schema": "https://vega.github.io/schema/vega-lite/v2.json",
  "description": "Stock price mean per quarter broken down by years.",
  "data": {
    "url": "data/stocks.csv"
  },
  "facet": {
    "column": {
      "field": "date",
      "type": "temporal",
      "timeUnit": "year"
    }
  },
  "spec": {
    "mark": "line",
    "encoding": {
      "x": {
        "timeUnit": "quarter",
        "field": "date",
        "type": "ordinal"
      },
      "y": {
        "field": "price",
        "type": "quantitative",
        "aggregate": "mean"
      },
      "color": {
        "field": "symbol",
        "type": "nominal"
      }
    }
  }
}